def send_verification_code(email, verification_code):
    message = f"Your verification code is: {verification_code}"
    msg = EmailMessage()
    msg.set_content(message)
    msg['Subject'] = 'Email Verification Code'
    msg['From'] = os.environ.get('EMAIL_ADDRESS')
